            "Action": [
                "logs:CreateLogStream",
                "logs:CreateLogGroup"
            ],
            "Resource": "arn:aws:logs:us-east-1:050404433262:log-group:/aws/lambda/lamda-practice-dev*:*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "logs:PutLogEvents",
            "Resource": "arn:aws:logs:us-east-1:050404433262:log-group:/aws/lambda/lamda-practice-dev*:*:*"
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "ses:*",
                "ses:SendRawEmail"
            ],
            "Resource": "*"
        }
    ]
}
